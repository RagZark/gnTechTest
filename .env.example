# .env.example
# Copie este arquivo para .env e configure com seus valores

# WeatherAPI (obrigatório - obtenha em https://www.weatherapi.com/)
OPENWEATHER_API_KEY=sua_chave_da_weatherapi_aqui

# API Key para sua API (defina uma chave secreta que será passada como parâmetro)
API_KEY=sua_chave_secreta_aqui

# Configurações do Banco (já configuradas no docker-compose)
DB_HOST=db
DB_PORT=5432
DB_NAME=weather_db
DB_USER=weather_user
DB_PASSWORD=weather_password

# Configurações do Servidor
PORT=3000
NODE_ENV=production
CORS_ORIGIN=http://localhost:3000